{
  "name": "backend",
  "version": "1.0.0",
  "description": "NestJS Backend",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "test": "vitest --config vitest.config.ts",
    "zip": "zip -r lambda.zip dist/ package.json",
    "upload-s3": "aws s3 cp lambda.zip s3://bcan-lambda-deployments/lambda.zip --region us-east-2",
    "deploy": "npm run build && npm run zip && npm run upload-s3 && aws lambda update-function-code --function-name bcanBackendLambda --s3-bucket bcan-lambda-deployments --s3-key lambda.zip --region us-east-2"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.5",
    "@vendia/serverless-express": "^4.12.6",
    "aws-jwt-verify": "^5.1.1",
    "aws-sdk": "^2.1693.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "jwt-decode": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/testing": "^11.1.12",
    "@types/aws-lambda": "^8.10.159",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/cookie-parser": "^1.4.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "aws-sdk": "^2.1693.0",
    "esbuild": "^0.27.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
